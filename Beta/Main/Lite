local OrionLib = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Orion/main/source"))()

-- Создаем окно
local Window = OrionLib:MakeWindow({Name = "No name Hub (BETA)", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionConfig"})

-- Вкладка Main
local mainTab = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Вкладка Murder
local murderTab = Window:MakeTab({
    Name = "Murder",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Вкладка Sheriff
local sheriffTab = Window:MakeTab({
    Name = "Sheriff",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

-- Вкладка Server
local serverTab = Window:MakeTab({
    Name = "Server",
    Icon = "rbxassetid://4483345998",
    PremiumOnly = false
})

local player = game.Players.LocalPlayer
local character = player.Character or player.CharacterAdded:Wait()
local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

-- Переменные для автофарма и ESP
local autoFarmEnabled = false
local espEnabled = false
local itemPickupDistance = 10 -- Максимальное расстояние для сбора предметов

-- Функция для автофарма
function startAutoFarm()
    autoFarmEnabled = true
    while autoFarmEnabled do
        wait(0.5)
        for _, item in ipairs(workspace:GetChildren()) do
            if item:IsA("Part") and item.Name == "Item" and (item.Position - humanoidRootPart.Position).magnitude <= itemPickupDistance then
                humanoidRootPart.CFrame = CFrame.new(item.Position + Vector3.new(0, 2, 0))
                wait(0.1)
                fireclickdetector(item:FindFirstChild("ClickDetector"))
                wait(0.2)
            end
        end
    end
end

function stopAutoFarm()
    autoFarmEnabled = false
end

-- Функция для переключения ESP
local function toggleESP(state)
    espEnabled = state
    for _, target in ipairs(game.Players:GetPlayers()) do
        if target.Character then
            if state then
                local highlight = Instance.new("Highlight")
                highlight.Parent = target.Character
                highlight.Adornee = target.Character
                highlight.FillColor = Color3.new(1, 0, 0) -- Красный цвет
                highlight.FillTransparency = 0.5
            else
                if target.Character:FindFirstChild("Highlight") then
                    target.Character.Highlight:Destroy()
                end
            end
        end
    end
end

-- Слайдер для изменения скорости в Main
mainTab:AddSlider({
    Name = "Speed",
    Min = 0,
    Max = 500,
    Default = 16,
    Color = Color3.fromRGB(255, 0, 0),
    Increment = 1,
    ValueName = "WalkSpeed",
    Callback = function(value)
        player.Character.Humanoid.WalkSpeed = value
    end
})

-- Кнопка "On/Off" для автофарма
mainTab:AddToggle({
    Name = "Auto Farm On/Off",
    Default = false,
    Callback = function(state)
        if state then
            startAutoFarm()
        else
            stopAutoFarm()
        end
    end
})

-- Кнопка "On/Off" для ESP
mainTab:AddToggle({
    Name = "ESP On/Off",
    Default = false,
    Callback = function(state)
        toggleESP(state)
    end
})

-- Кнопки для перемещения игрока
mainTab:AddButton({
    Name = "Move Forward",
    Callback = function()
        humanoidRootPart.CFrame = humanoidRootPart.CFrame + humanoidRootPart.CFrame.LookVector * 10
    end
})

mainTab:AddButton({
    Name = "Move Backward",
    Callback = function()
        humanoidRootPart.CFrame = humanoidRootPart.CFrame - humanoidRootPart.CFrame.LookVector * 10
    end
})

mainTab:AddButton({
    Name = "Move Left",
    Callback = function()
        humanoidRootPart.CFrame = humanoidRootPart.CFrame - humanoidRootPart.CFrame.RightVector * 10
    end
})

mainTab:AddButton({
    Name = "Move Right",
    Callback = function()
        humanoidRootPart.CFrame = humanoidRootPart.CFrame + humanoidRootPart.CFrame.RightVector * 10
    end
})

-- Функция для автоматического убийства всех игроков, если вы убийца
local function killAllPlayers()
    if player.Backpack:FindFirstChild("Knife") or character:FindFirstChild("Knife") then
        local knife = player.Backpack:FindFirstChild("Knife") or character:FindFirstChild("Knife")
        if knife then
            knife.Parent = character
        end

        for _, target in ipairs(game.Players:GetPlayers()) do
            if target ~= player and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
                target.Character.HumanoidRootPart.CFrame = humanoidRootPart.CFrame + Vector3.new(0, 2, 0)
                wait(0.2)
                mouse1click()
            end
        end
    else
        OrionLib:MakeNotification({
            Name = "Ошибка",
            Content = "Вы не убийца!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

-- Кнопка "Kill All" в Murder
murderTab:AddButton({
    Name = "Kill All",
    Callback = function()
        killAllPlayers()
    end
})

-- Функция для сбора пистолета
sheriffTab:AddButton({
    Name = "Grab Gun",
    Callback = function()
        for _, item in ipairs(workspace:GetChildren()) do
            if item:IsA("Tool") and item.Name == "Gun" then
                item.Handle.CFrame = humanoidRootPart.CFrame
                return
            end
        end
        OrionLib:MakeNotification({Name = "Ошибка", Content = "Пистолет не найден!", Time = 5})
    end
})

-- Копирование ID сервера
serverTab:AddButton({
    Name = "Copy Server ID",
    Callback = function()
        setclipboard(game.JobId)
        OrionLib:MakeNotification({
            Name = "Сервер ID",
            Content = "ID сервера скопирован в буфер обмена!",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

-- Ввод ID сервера
serverTab:AddTextbox({
    Name = "Server ID",
    PlaceholderText = "Введите Server ID",
    Callback = function(value)
        local TeleportService = game:GetService("TeleportService")
        TeleportService:TeleportToPlaceInstance(game.PlaceId, value, player)
    end
})

-- Кнопка для телепортации на сервер по ID
serverTab:AddButton({
    Name = "Teleport to Server",
    Callback = function()
        local id = getgenv().serverId or ""
        if id ~= "" then
            teleportToServer(id)
        else
            OrionLib:MakeNotification({
                Name = "Ошибка",
                Content = "Введите действительный ID сервера.",
                Image = "rbxassetid://4483345998",
                Time = 5
            })
        end
    end
})

-- Функция для серверного хопа
local function serverHop()
    local TeleportService = game:GetService("TeleportService")
    TeleportService:Teleport(game.PlaceId, player)
end

-- Кнопка "Server Hop" в Server
serverTab:AddButton({
    Name = "Server Hop",
    Callback = function()
        serverHop()
    end
})

-- Функция для поиска маленького сервера
local function findSmallServer()
    local success, message = pcall(function()
        local TeleportService = game:GetService("TeleportService")
        local placeId = game.PlaceId
        local servers = TeleportService:GetPrivateServers(placeId)

        for _, server in ipairs(servers) do
            if server.PlayerCount < 5 then -- Измените количество игроков по своему усмотрению
                TeleportService:TeleportToPlaceInstance(placeId, server.Id, player)
                return
            end
        end
    end)

    if not success then
        OrionLib:MakeNotification({
            Name = "Ошибка",
            Content = "Не удалось найти маленький сервер.",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
end

-- Кнопка "Small Server" в Server
serverTab:AddButton({
    Name = "Small Server",
    Callback = function()
        findSmallServer()
    end
})

-- Запуск интерфейса
OrionLib:Init()
